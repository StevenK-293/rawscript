{
  "version": 2,
  "routes": [
    {
      "src": "/api/raw/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/raw/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
